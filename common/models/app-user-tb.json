{
  "name": "AppUser_TB",
  "plural": "AppUsers_TB",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "number",
      "required": true
    },
    "LastName": {
      "type": "string",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "IsMilitar": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "TimeCreate": {
      "type": "date",
      "required": true
    },
    "isTemporal": {
      "type": "boolean",
      "required": true,
      "default": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "prototype.registrar_usuario"
    }
  ],
  "methods": {
    "prototype.registrar_usuario": {
      "accepts": [
        {
          "arg": "data",
          "type": "object",
          "required": true,
          "description": "datos del registro"
        }
      ],
      "returns": [
        {
          "arg": "result",
          "type": "object",
          "root": true,
          "description": "datos almacenados"
        }
      ],
      "description": "Permitira registrar los datos basicos del usuario, una identificacion y informacion de contacto.",
      "http": [
        {
          "path": "/registro_usuario",
          "verb": "post"
        }
      ]
    }
  }
}
